    "karma-webpack": "^2.0.1",
    "minimist": "^1.2.0",
    "nunjucks": "^3.0.0",
    "tslint": "^5.5.0",
    "tslint-eslint-rules": "^4.1.1",
    "webpack-dev-server": "^2.2.0"
